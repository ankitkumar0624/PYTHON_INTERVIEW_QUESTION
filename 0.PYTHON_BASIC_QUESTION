### CHeck If Two String are anagram

s1="ankitw"
s2="kitan"
flag=True
if(len(s1)==len(s2)):
  for i in s1:
    if i not in s2:
      flag=False
      break
else:
  flag =False
if flag:
  print('YES')
else:
  print('NO')


### find most frequency character in a string
s="programming"
d={}
for i in s:
  if i in d:
    d[i]=d[i]+1
  else:
    d[i]=1
print(d)
max_count=-2**63
max_char=""
for i,j in d.items():
  if j > max_count:
    max_count = j
    max_char = i 
print(max_count,max_char)



## merge two sorted array
L1=[1,2,3,11,12]
L2=[0,4,9,10]
merge_list =[]
i,j=0,0
while i <len(L1) and j<len(L2) :
  if( L1[i]>L2[j]):
    merge_list.append(L2[j])
    j+=1
  elif (L1[i]==L2[j]) :
    merge_list.append(L1[i])
    merge_list.append(L2[j])
    i+=1
    j+=1
  else :
    merge_list.append(L1[i])
    i+=1
merge_list+=(L1[i:])
merge_list+=(L2[j:])
print(merge_list)



### Sort the List without using sorting function.
del L
L = [4,10,8,9]
for i in range(0,len(L)):
  for j in range(i+1,len(L)):
    if(L[i]>L[j]):
      L[i],L[j]=L[j],L[i]
print(L)

## find the second largest element
L=[8,4,10,9]
first=-2**63
second =-2**63
for num in L:
  if num>first:
    second=first
    first=num
  elif num > second and num !=first:
    second=num
print(second)

## find sum digits of number
num=1111111111111111111111122222222222222222222222222222333333333333333333333334444444444444444444444444444
num=str(num)
num_list =list(num)
print(num_list)
sum=0
for i in num_list:
  sum =sum+int(i)
print(sum)
num1=12
sum1=0
while num1>0:
  sum1+=num1%10
  num1=num1//10
print(sum1)


## reverse a string
s="ankit"
s=list(s)
r=len(s)-1
l=0
while r>l :
  t=s[l]
  s[l]=s[r]
  s[r]=t
  r-=1
  l+=1
print(s)
s="".join(s)
print(s)

s="python"
r=""

for i in s:
  r=i+r
print(s,r)


## vowels and consonants count check
s="ankit"
vowels=0
cons=0
for i in s:
  if i in ['a','e','i','o','u']:
    vowels+=1
  else :
    cons+=1
print(vowels,cons)


## palindrome check
s="ankit"
r=len(s)-1
l=0
# print(len(s))
flag=True
while(r>=l):
    if(s[r]!=s[l]):
      flag = False
      break
    r-=1
    l+=1
if flag :
  print("Palindrome")
else :
  print("NOT an Palindrome")
    
##num = int(input("input an number to check whether is even or odd :: "))
num=5
if num%2==0:
  print('EVEN')
else :
  print('ODD')

##num =int(input("find factorial of num ::"))
num=10
fac=1
for i in range(1,num+1):
  fac*=i
print(fac)












